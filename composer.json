{
    "name": "inspheric/nova-indicator-field",
    "description": "A Laravel Nova indicator field.",
    "keywords": [
        "laravel",
        "nova",
        "nova-4",
        "field",
        "status",
        "indicator"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Inspheric\\Fields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inspheric\\Fields\\IndicatorFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
